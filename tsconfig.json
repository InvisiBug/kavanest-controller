{
  "compilerOptions": {
    "target": "ESNext",
    // "module": "ES6",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noEmitOnError": true,
    "noUnusedLocals": false, // ! Turn this back on later
    "noUnusedParameters": false,
    "resolveJsonModule": true,
    "strict": true,
    "declaration": true,
    "outDir": "dist/",
    // "types": ["jest", "node", "@types/jest"],
    "typeRoots": [
    "./node_modules/@types/",
    "./src/@types/"
    ]
  },
  "include": [
    ".",
  ]
}